  "projects": {
    "todo-app": { "tags": [] },
    "todo-app-e2e": { "tags": [], "implicitDependencies": ["todo-app"] },
    "todo-util": { "tags": [] },
    "todo-shared": { "tags": [] },
    "todo-public": { "tags": [] }
  }
}
